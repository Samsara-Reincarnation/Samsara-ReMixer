Script "DetermineValidSpawnReMixer" Open
{
	if(GetCVar("mm_generate") <= 0 && GetCVar("mm_reducemonsters") >= 99)
		terminate;

	until(GetCVar("samsara_processedgame")) { Delay(1); }

	ACS_NamedExecuteWithResult("SMMGenerator_DetermineGameChoice");
	processedCounts = 1;

	str cellCheckClass = GetCVar("samsara_zscriptready") == 1 ? "DetermineValidCellReMixerZS" : "DetermineValidCellReMixer";

	for (int a = 512; a <= 0.5; a += 1024)
	{
		int fixedA = a * 1.0;

		for (int b = 512; b <= 0.5; b += 1024)
		{
			int fixedB = b * 1.0;

			SpawnForced(cellCheckClass, fixedA, fixedB, 0, 0);

			if (b > 0)
				b = -b - 1024;
			else
				b = -b;
		}

		if (a > 0)
			a = -a - 1024;
		else
			a = -a;
	}
}

Script "SMMGenerator_DetermineGameChoice" (void)
{
	int game = GetCVar("samsara_currentgame");
	int selector, currentCounterForSelector;
	switch(game)
	{
		case 1:
			for(selector = 0; selector < MAXMONSTERS_DOOM; selector++)
			{
				currentCounterForSelector = ThingCountName(SpawnersDoom[selector],0);
				generatorCountersDoom[selector] = currentCounterForSelector;

				if(currentCounterForSelector > 0)
				{
					minSelector = min(selector, minSelector);
					maxSelector = max(selector, maxSelector);
				}
				totalTypes += currentCounterForSelector;
				generationAmount += currentCounterForSelector*(selector+1);
			}

			spawnSecretMonsters = generatorCountersDoom[3] > 0;
			break;

		case 2:
			// Check if we're playing Chex Quest 3 or Chex Quest 1
			int CQ1CheckTid = UniqueTid();

			if(!SpawnForced("FlemoidusCommonus2_CQ1",0,0,0,CQ1CheckTid))
			{
				// We're playing Chex Quest 3
				for(selector = 0; selector < MAXMONSTERS_CHEX; selector++)
				{
					currentCounterForSelector = ThingCountName(SpawnersChex[selector],0);
					generatorCountersChex[selector] = currentCounterForSelector;

					if(currentCounterForSelector > 0)
					{
						minSelector = min(selector, minSelector);
						maxSelector = max(selector, maxSelector);
					}
					totalTypes += currentCounterForSelector;
					generationAmount += currentCounterForSelector*(selector+1);
				}
				break;
			}
			else
			{
				// We're playing Chex Quest 1
				Thing_Remove(CQ1CheckTid);

				for(selector = 0; selector < MAXMONSTERS_CHEXORIGINAL; selector++)
				{
					currentCounterForSelector = ThingCountName(SpawnersChexOriginal[selector],0);
					generatorCountersChexOriginal[selector] = currentCounterForSelector;

					if(currentCounterForSelector > 0)
					{
						minSelector = min(selector, minSelector);
						maxSelector = max(selector, maxSelector);
					}
					totalTypes += currentCounterForSelector;
					generationAmount += currentCounterForSelector*(selector+1);
				}
				break;
			}

		case 4:
			for(selector = 0; selector < MAXMONSTERS_HERETIC; selector++)
			{
				currentCounterForSelector = ThingCountName(SpawnersHeretic[selector],0);
				generatorCountersHeretic[selector] = currentCounterForSelector;

				if(currentCounterForSelector > 0)
				{
					minSelector = min(selector, minSelector);
					maxSelector = max(selector, maxSelector);
				}
				totalTypes += currentCounterForSelector;
				generationAmount += currentCounterForSelector*(selector+1);
			}
			break;

		case 8:
			for(selector = 0; selector < MAXMONSTERS_HEXEN; selector++)
			{
				currentCounterForSelector = ThingCountName(SpawnersHexen[selector],0);
				generatorCountersHexen[selector] = currentCounterForSelector;

				if(currentCounterForSelector > 0)
				{
					minSelector = min(selector, minSelector);
					maxSelector = max(selector, maxSelector);
				}
				totalTypes += currentCounterForSelector;
				generationAmount += currentCounterForSelector*(selector+1);
			}
			break;

		case 16:
			for(selector = 0; selector < MAXMONSTERS_STRIFE; selector++)
			{
				currentCounterForSelector = ThingCountName(SpawnersStrife[selector],0);
				generatorCountersStrife[selector] = currentCounterForSelector;

				if(currentCounterForSelector > 0)
				{
					minSelector = min(selector, minSelector);
					maxSelector = max(selector, maxSelector);
				}
				totalTypes += currentCounterForSelector;
				generationAmount += currentCounterForSelector*(selector+1);
			}
			break;

		case 32:
			for(selector = 0; selector < MAXMONSTERS_HACX; selector++)
			{
				currentCounterForSelector = ThingCountName(SpawnersHacx[selector],0);
				generatorCountersHacx[selector] = currentCounterForSelector;

				if(currentCounterForSelector > 0)
				{
					minSelector = min(selector, minSelector);
					maxSelector = max(selector, maxSelector);
				}
				totalTypes += currentCounterForSelector;
