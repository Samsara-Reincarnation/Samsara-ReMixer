			break;

		case 64:
			for(selector = 0; selector < MAXMONSTERS_HARMONY; selector++)
			{
				currentCounterForSelector = ThingCountName(SpawnersHarmony[selector],0);
				generatorCountersHarmony[selector] = currentCounterForSelector;

				if(currentCounterForSelector > 0)
				{
					minSelector = min(selector, minSelector);
					maxSelector = max(selector, maxSelector);
				}
				totalTypes += currentCounterForSelector;
				generationAmount += currentCounterForSelector*(selector+1);
			}
			break;

		case 128:
			for(selector = 0; selector < MAXMONSTERS_ROTWB; selector++)
			{
				currentCounterForSelector = ThingCountName(SpawnersROTWB[selector],0);
				generatorCountersROTWB[selector] = currentCounterForSelector;

				if(currentCounterForSelector > 0)
				{
					minSelector = min(selector, minSelector);
					maxSelector = max(selector, maxSelector);
				}
				totalTypes += currentCounterForSelector;
				generationAmount += currentCounterForSelector*(selector+1);
			}
			break;

		case 256:
			for(selector = 0; selector < MAXMONSTERS_REKKR; selector++)
