//For advanced sight behavior
Actor RTCW_Sight : CustomInventory
{
	-COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	Inventory.PickupMessage ""
	Inventory.PickupSound ""
	States
	{
		Pickup:
			TNT1 A 0
			//Regrettably, RTCW AI is too complex for slaughtermaps
			//TNT1 A 0 A_JumpIf(CallACS("RTCW_Decorate",19)==1,6)
			//As a note, A_LookEx picks on the opposite of a first come, first serve basis, which is why the higher priority checks are last
			TNT1 A 0 A_LookEx(LOF_NOSIGHTCHECK,0,2048,2048,360,"QueryHear")
			TNT1 A 0 A_LookEx(LOF_NOSOUNDCHECK,0,2048,2048,180,"QuerySee")
			TNT1 A 0 A_LookEx(LOF_NOSIGHTCHECK,0,1024,1024,180,"InstantAlert")
			TNT1 A 0 A_LookEx(LOF_NOSIGHTCHECK,0,384,384,360,"InstantCombat")
			TNT1 A 0 A_LookEx(0,0,192,192,180,"InstantCombat")
			TNT1 A 0
			stop
		//These are just kept for Zandronum, because GZDoom actually does change states from custom inventories.
		QueryHear:
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_Decorate",20)
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState")
			Stop
		QuerySee:
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_Decorate",20)
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,1)
			Stop
		InstantAlert:
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_Decorate",20)
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,2)
			Stop
		InstantCombat:
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_Decorate",20)
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,3)
			Stop
	}
}

//For querying
Actor RTCW_QuerySight : CustomInventory
{
	-COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	Inventory.PickupMessage ""
	Inventory.PickupSound ""
	States
	{
		Pickup:
			TNT1 A 0
			TNT1 A 0 A_JumpIfTargetInLOS(3,180,JLOSF_DEADNOJUMP,2048)
			TNT1 A 0 A_LookEx(LOF_NOSIGHTCHECK|LOF_NOJUMP,0,2048,2048,360,"QueryToAlert")
			TNT1 A 0 A_LookEx(LOF_NOSOUNDCHECK|LOF_NOJUMP,0,2048,2048,180,"QuerySight")
			Goto NoTarget
			TNT1 A 0 A_JumpIf(CallACS("RTCW_Decorate",10)==1,1)
			Goto NoTarget
		QuerySight:
			TNT1 A 0 A_JumpIfInventory("RTCW_SightThreshold",16,"QueryToAlert")
			TNT1 A 0 A_GiveInventory("RTCW_SightThreshold",1)
			stop
		NoTarget:
			TNT1 A 0
			stop
		QueryToAlert:
			TNT1 A 0 A_FaceTarget
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,4)
			Stop
	}
}

//For alert sight, they are aware of you, but they do not know where to look. So they will look for you
Actor RTCW_AlertSight : CustomInventory
{
	-COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	Inventory.PickupMessage ""
	Inventory.PickupSound ""
	States
	{
		Pickup:
			TNT1 A 0
			TNT1 A 0 A_LookEx(LOF_NOSIGHTCHECK|LOF_NOJUMP,0,2048,2048,360,"Combat")
			TNT1 A 0 A_LookEx(LOF_NOSOUNDCHECK|LOF_NOJUMP,0,2048,2048,360,"Combat")
			TNT1 A 0 A_JumpIfTargetInLOS("Combat",360,JLOSF_DEADNOJUMP,2048)
			TNT1 A 0 A_JumpIfInventory("RTCW_AlertRunning",1,"Run")
			TNT1 A 0 A_JumpIfInventory("RTCW_AlertWalking",1,"Walk")
			TNT1 A 0 A_AlertMonsters(512)
			stop
		Combat:
			TNT1 A 0 A_TakeInventory("RTCW_AlertWalking",1)
			TNT1 A 0 A_TakeInventory("RTCW_AlertRunning",1)
			TNT1 A 0 A_Jump(128,2)
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,3)
			Stop
		Walk:
			TNT1 A 0 A_Warp(AAPTR_Default,4,0,0,0,WARPF_TESTONLY,1)
			Goto Turn
			TNT1 A 0 A_ChangeVelocity(1.5,0,velz,CVF_RELATIVE|CVF_REPLACE)
			Stop
		Run:
			TNT1 A 0 A_Warp(AAPTR_Default,4,0,0,0,WARPF_TESTONLY,1)
			Goto Turn
			TNT1 A 0 A_ChangeVelocity(3,0,velz,CVF_RELATIVE|CVF_REPLACE)
			Stop
		Turn:
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,random(5,6))
			Stop
	}
}

Actor RTCW_CombatThink : CustomInventory
{
	-COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	Inventory.PickupMessage ""
	Inventory.PickupSound ""
	States
	{
		Pickup:
			TNT1 A 0
			TNT1 A 0 A_JumpIf(CallACS("RTCW_Decorate",12)==1,"Dismiss")
			TNT1 A 0 A_JumpIfTargetInLOS(1,0,JLOSF_DEADNOJUMP,2048)
			Goto Pickup+4
			TNT1 A 0 A_FaceTarget(15)
			TNT1 A 0 A_JumpIfInventory("RTCW_Enemy_MoveOrder",5,5)
			TNT1 A 0 A_JumpIfInventory("RTCW_Enemy_MoveOrder",4,"StrafeRight")
			TNT1 A 0 A_JumpIfInventory("RTCW_Enemy_MoveOrder",3,"StrafeLeft")
			TNT1 A 0 A_JumpIfInventory("RTCW_Enemy_MoveOrder",2,"RunBackward")
			TNT1 A 0 A_JumpIfInventory("RTCW_Enemy_MoveOrder",1,"RunForward")
			TNT1 A 0 A_TakeInventory("RTCW_Enemy_MoveOrder",8)
			TNT1 A 0 A_ChangeVelocity(0,0,velz,CVF_REPLACE)
			TNT1 A 0 A_AlertMonsters(512)
			TNT1 A 0
			Stop
		RunForward:
			TNT1 A 0 A_Warp(AAPTR_Default,4,0,0,0,WARPF_TESTONLY,1)
			Goto CombatStand
			TNT1 A 0 A_FaceTarget(random(1,15))
			TNT1 A 0 A_ChangeVelocity(3,0,velz,CVF_RELATIVE|CVF_REPLACE)
			Stop
		RunBackward:
			TNT1 A 0 A_Warp(AAPTR_Default,-4,0,0,0,WARPF_TESTONLY,1)
			Goto CombatStand
			TNT1 A 0 A_FaceTarget(random(1,15))
			TNT1 A 0 A_ChangeVelocity(-3,0,velz,CVF_RELATIVE|CVF_REPLACE)
			Stop
		StrafeLeft:
			TNT1 A 0 A_Warp(AAPTR_Default,0,-4,0,0,WARPF_TESTONLY,1)
			Goto CombatStand
			TNT1 A 0 A_FaceTarget(random(1,15))
			TNT1 A 0 A_ChangeVelocity(0,-3,velz,CVF_RELATIVE|CVF_REPLACE)
			Stop
		StrafeRight:
			TNT1 A 0 A_Warp(AAPTR_Default,0,4,0,0,WARPF_TESTONLY,1)
			Goto CombatStand
			TNT1 A 0 A_FaceTarget(random(1,15))
			TNT1 A 0 A_ChangeVelocity(0,3,velz,CVF_RELATIVE|CVF_REPLACE)
			Stop
		CombatStand:
			TNT1 A 0 A_SpawnItemEx("RTCW_DoorUser",radius,0,0,0,0,0,angle,SXF_NOCHECKPOSITION|SXF_TRANSFERPOINTERS)
			TNT1 A 0 A_JumpIfTargetInLOS(1,360,JLOSF_DEADNOJUMP,2048)
			Goto CombatTurn
			TNT1 A 0 A_TakeInventory("RTCW_Enemy_MoveOrder",8)
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,9)
			Stop
		CombatTurn:
			TNT1 A 0 A_TakeInventory("RTCW_Enemy_MoveOrder",8)
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,10)
			Stop
		Dismiss:
			TNT1 A 0 A_ClearTarget
			TNT1 A 0 A_TakeInventory("RTCW_Enemy_MoveOrder",8)
			TNT1 A 0 ACS_NamedExecuteWithResult("RTCW_EnemyState",0,8)
			Stop
	}
}

